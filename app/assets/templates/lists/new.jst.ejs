<form class="new-list">
  <label for="list_title"></label>
  <input type="text" id="list_title" name="list[title]" placeholder="Add a list...">

  <input type="hidden" name="list[board_id]" value="<%= board.id %>">
  <input type="hidden" name="list[ord]" placeholder="<%= board.nextOrd() %>">

  <input id="submit-new-list" type="submit" placeholder="Save">
</form>
